@article{derrico2017identifying,
  title = {Identifying early modern human ecological niche expansions and associated cultural dynamics in the {{South African Middle Stone Age}}},
  author = {family=Errico, given=Francesco, prefix=d’, useprefix=true and Banks, William E. and Warren, Dan L. and Sgubin, Giovanni and family=Niekerk, given=Karen, prefix=van, useprefix=true and Henshilwood, Christopher and Daniau, Anne-Laure and Sánchez Goñi, María Fernanda},
  date = {2017-07-25},
