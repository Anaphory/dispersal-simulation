@article{arita1998evolution,
  title = {Evolution of {{Linguistic Diversity}} in a {{Simple Communication System}}},
  author = {Arita, Takaya and Koyama, Yuhji},
  date = {1998-01-01},
  journaltitle = {Artificial Life},
