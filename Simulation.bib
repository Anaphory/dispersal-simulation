@article{javarone2014competitive,
  title = {Competitive dynamics of lexical innovations in multi-layer networks},
  author = {Javarone, Marco Alberto},
  date = {2014-03-11},
  journaltitle = {International Journal of Modern Physics C},
  shortjournal = {Int. J. Mod. Phys. C},
