  title = {Social {{Interaction}} in {{Hunter-Gatherer Societies}}: {{Simulating}} the {{Consequences}} of {{Cooperation}} and {{Social Aggregation}}},
  shorttitle = {Social {{Interaction}} in {{Hunter-Gatherer Societies}}},
  author = {Barceló, Juan A. and Bernal, Florencia Del Castillo and family=Olmo, given=Ricardo, prefix=del, useprefix=false and Mameli, Laura and Quesada, F. J. Miguel and Poza, David and Vilà, Xavier},
