@article{banks2006ecocultural,
  ids = {banksecocultural},
  title = {Eco-{{Cultural Niche Modeling}}: {{New Tools}} for {{Reconstructing}} the {{Geography}} and {{Ecology}} of {{Past Human Populations}}},
  shorttitle = {Eco-{{Cultural Niche Modeling}}},
  author = {Banks, William E. and D'Errico, F. and Dibble, Harold L. and Krishtalka, Leonard and West, Dixie and Olszewski, Deborah and Townsend Peterson, A. and Anderson David, G. and Gillam, G. Christopher and Montet-White, Anta and Crucifix, Michel and Marean, Curtis W. and Sánchez-Goñi, María-Fernanda and Wohlfarthh, Barbara and Vanhaeren, Marian},
  date = {2006},
  journaltitle = {PaleoAnthropology},
  volume = {2006},
  pages = {68--83},
  url = {https://halshs.archives-ouvertes.fr/halshs-00452486},
  urldate = {2020-05-11},
  abstract = {Prehistoric human populations were influenced by climate change and resulting environmental variability and developed a wide variety of cultural mechanisms to deal with these conditions. In an effort to understand the inesfluence of environmental factors on prehistoric social and technical systems, there is a need to establish methods with which to model and evaluate the rules and driving forces behind these human-environment interactions. We describe a new set of analytical tools―an approach termed Eco-Cultural Niche Modeling (ECNM)―that can be used to address these issues and to test current hypotheses. This approach's modeling architectures are used to reconstruct past human systems in the Old and New Worlds, past natural systems within which they operated― namely geological, paleobiological and paleoenvironmental conditions―and also to develop informed hypotheses concerning the geographic spread, migration, and eco-cultural adaptations of prehistoric human populations. The ECNM approach has recently been developed and explored at two National Science Foundation- and European Science Foundation-funded workshops. We describe the goals and methods of ECNM, the results of the proof-of-concept projects, the analytical issues that remain unresolved, and the potential this approach has to offer the disciplines of paleoanthropology and archaeology.},
  keywords = {ecology,Geography,output,Past Human Populations},
  file = {/home/gereon/Zotero/storage/EI27MVWA/Banks - Eco-Cultural Niche Modeling New Tools for Reconst.pdf;/home/gereon/Zotero/storage/C4AYEZ9L/halshs-00452486.html}
}
